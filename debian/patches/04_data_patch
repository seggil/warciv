+[building_cathedral]
+name		= _("Cathedral")
+tech_req	= "Monotheism"
+bldg_req	= "None"
+graphic	= "b.cathedral"
+graphic_alt	= "-"
+;terr_gate	=
+;spec_gate	=
+equiv_range	= "City"

+graphic_alt	= "-"
+;terr_gate	=
+;spec_gate	=
+equiv_range	= "City"
+;equiv_dupl	=
+;equiv_repl	=
+obsolete_by	= "None"
+is_wonder	= 0
+build_cost	= 999
+upkeep		= 0
+sabotage	= 0
+effect		=
+    { "name"
+      "Prod_To_Gold"
+    }
+; FIXME: this is the real helptext; restore when have a subordnate analogue
+; /* (ignore for gettext until fixed)

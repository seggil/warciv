[nation_basque]

name=_("Basque")
plural=_("?plural:Basques")
groups="Medieval", "European"
class = _("European")
legend=_("The Basques have lived since times immemorial by the western\
 foothills of the Pyrenees.\
 By maintaining their age-old cultural traditions, and keeping their\
 historic identity alive, the Basques provide a link with man's distant\
 past. Euskara, the language spoken by the Basque people, is probably the\
 oldest living language in Europe.")
leader=
 "Antso III.a Gartzes Nagusia",
 "Eneko Aritza",
 "José Antonio Aguirre",
 "Sabino Arana",
 "Fernando Buesa",
 "Patxi Iturrioz",
 "Federico Krutwig",
 "Jesús María de Leizaola",
 "Mario Onaindia",
 "Ramón Rubial Cavia",
 "Tomas Zumalakarregi Imaz"

leader_sex="Male", "Male", "Male", "Male", "Male", "Male", "Male",
           "Male", "Male", "Male", "Male"
flag="f.euskadi"
flag_alt = "-"
city_style = "Celtic"

ruler_titles = { "government",     "male_title",        "female_title"
; /* TRANS: Buruzagi = Leader, head (like spanish Caudillo) */
                 "Despotism",      _("Buruzagi"),       _("?female:Buruzagi")
; /* TRANS: Apaiz nagusi = Pope, Lord Protector */
               "Fundamentalism", _("Apaiz nagusi"), _("Emakume Apaiz nagusi")
; /* TRANS: Errege and Erregina = King and Queen */
                 "Monarchy",       _("Errege"),           _("Erregina")
; /* TRANS: Lehendakari = similar to President 
; /* TRANS: (but is quite inexact for Basques to call him/her "president") */
                 "Democracy",      _("Lehendakari"), _("?female:Lehendakari")
                 "Republic",       _("Lehendakari"), _("?female:Lehendakari")
               }

init_techs=""
init_buildings=""
init_government="Despotism"
init_units=""

civilwar_nations="spanish", "french"


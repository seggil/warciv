"    N = visa befolkning\n"
"    P = visa tillverkning\n"
"    A = visa landyta\n"
"    L = visa läskunnighet\n"
"    R = visa forskningshastighet\n"
"    S = visa bosatt yta\n"
"    E = visa ekonomi\n"
"    M = visa militärtjänst\n"
"    O = visa utsläpp\n"

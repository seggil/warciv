#, c-format
msgid ""
"Usage: %s [option ...]\n"
"Valid options are:\n"
msgstr ""
"Použití: %s [volba ...]\n"
"Možné volby jsou:\n"

msgid "  -a, --autoconnect\tSkip connect dialog\n"
msgstr "  -a, --autoconnect\tVynechat připojovací dialog\n"

#, fuzzy
msgid ""
"  -d, --debug NUM\tSet debug log level (0 to 4, or 4:file1,min,max:... or 4:"
